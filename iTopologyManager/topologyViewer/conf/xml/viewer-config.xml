        <data key="deviceModel">passiveHub</data>
        <data key="ipv6Forwarding">YES</data>
    </icon>

    <icon name="/images/subnet.png">
        <data key="deviceModel">passiveHub</data>
    </icon>
    <!--UNKNOWN-->

    <icon name="/images/unknown_switch.png,/images/circle_red.png">
