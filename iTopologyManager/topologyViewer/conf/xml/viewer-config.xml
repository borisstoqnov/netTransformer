    </icon>
    <icon name="/images/unknown_switch.png">
        <data key="deviceType">UNKNOWN</data>
    </icon>
    <icon name="/images/computer.png">
    </icon>

    <filters>
        <filter name="IPv4" type="or">

            <include for="edge"/>
            <include dataKey="ipv4Forwarding" dataValue="YES" for="node"/>
            <include dataKey="deviceType" dataValue="Subnet" for="node"/>
            <tooltip dataKey="nodeInfo" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.DefaultNodeTooltipTransformer"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="IPv6" type="or">

            <include for="edge"/>
            <include dataKey="ipv6Forwarding" dataValue="YES" for="node"/>
            <include dataKey="deviceType" dataValue="Subnet" for="node"/>
            <tooltip dataKey="nodeInfo" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.DefaultNodeTooltipTransformer"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="IPv4+IPv6" type="or">

            <include for="edge"/>
            <include dataKey="ipv4Forwarding" dataValue="YES" for="node"/>
            <include dataKey="ipv6Forwarding" dataValue="YES" for="node"/>
            <include dataKey="deviceType" dataValue="Subnet" for="node"/>
            <tooltip dataKey="nodeInfo" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.DefaultNodeTooltipTransformer"/>
            <tooltip dataKey="method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="all" type="or">

            <include for="edge"/>
            <include for="node"/>
            <tooltip dataKey="nodeInfo" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.DefaultNodeTooltipTransformer"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>
        </filter>




        <!--In this case is presented a physical view with all devices-->



        <filter name="diff" type="or">
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <include dataKey="Discovery Method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="Discovery Method" dataValue="CDP" for="edge"/>
            <include dataKey="Discovery Method" dataValue="LLDP" for="edge"/>
            <include dataKey="Discovery Method" dataValue="MAC" for="edge"/>
            <include dataKey="Discovery Method" dataValue="Slash30" for="edge"/>
            <include dataKey="Discovery Method" dataValue="Slash31" for="edge"/>
            <include dataKey="Discovery Method" dataValue="OSPF" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_ISIS" for="edge"/>
            <include dataKey="Discovery Method" dataValue="r_ISIS" for="edge"/>
            <include dataKey="Discovery Method" dataValue="r_OSPF" for="edge"/>
            <include dataKey="Discovery Method" dataValue="r_STATIC_ROUTE" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="diffs" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.HTMLCSVNodeTooltipTransformer"/>
            <tooltip dataKey="diffs" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>

        <!--In this case is presented a view only in site with Cisco and Default type Devices-->
        <filter name="CISCO Devices" type="or">
            <include dataKey="deviceType" dataValue="CISCO" for="node"/>
            <include for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>

        <!--In this case is presented a only CDP view -->
        <filter name="CDP" type="and">
            <include dataKey="Discovery Method" dataValue="CDP" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only an OSPF view -->
        <filter name="OSPF" type="or">
            <include dataKey="Discovery Method" dataValue="OSPF" for="edge"/>
            <include dataKey="Discovery Method" dataValue="cOSPF" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_OSPF" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only an ISIS view -->
        <filter name="ISIS" type="or">
            <include dataKey="Discovery Method" dataValue="ISIS" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_ISIS" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only BGP view -->
        <filter name="BGP" type="or">
            <include dataKey="Discovery Method" dataValue="BGP" for="edge"/>
            <include dataKey="Discovery Method" dataValue="c_BGP" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>

        </filter>
        <filter name="IPLinkLayer-reachable" type="and">
            <include dataKey="ipLink" dataValue="YES" for="edge"/>
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.CSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="IPLinkLayer-reachable+unreachable" type="and">
            <include dataKey="ipLink" dataValue="YES" for="edge"/>
            <include for="node"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="Discovery Method" for="edge" transformer="net.itransformers.topologyviewer.nodetooltip.DefaultNodeTooltipTransformer"/>
        </filter>
    </filters>

