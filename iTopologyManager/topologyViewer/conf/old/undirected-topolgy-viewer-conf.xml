    <filters>
        <!--In this case is presented a physical view with devices that respond to SNMP-->
        <filter name="Physical-view">
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <!--include dataKey="deviceStatus" dataValue="initial" for="node"/-->
            <!--include dataKey="site" dataValue="" for="node"/-->
            <include dataKey="method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <include dataKey="method" dataValue="MAC" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <include dataKey="method" dataValue="Slash30" for="edge"/>
            <include dataKey="method" dataValue="Slash31" for="edge"/>
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
        </filter>
        <!--In this case is presented a physical view with all devices-->
        <filter name="Physical-view - all">
            <include dataKey="method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <include dataKey="method" dataValue="Slash30" for="edge"/>
            <include dataKey="method" dataValue="Slash31" for="edge"/>
            <include dataKey="method" dataValue="MAC" for="edge"/>
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <tooltip dataKey="deviceInfo" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.HTMLCSVNodeTooltipTransformer"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>

        <!--In this case is presented a view only in site with side-id = HP-->

        <filter name="diff-reachable">
            <include dataKey="method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <include dataKey="method" dataValue="MAC" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <include dataKey="method" dataValue="Slash30" for="edge"/>
            <include dataKey="method" dataValue="Slash31" for="edge"/>
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <tooltip dataKey="diffs" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.HTMLCSVNodeTooltipTransformer"/>
            <tooltip dataKey="diffs" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="diff-any">
            <include dataKey="method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <include dataKey="method" dataValue="Slash30" for="edge"/>
            <include dataKey="method" dataValue="Slash31" for="edge"/>
            <include dataKey="method" dataValue="MAC" for="edge"/>
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <tooltip dataKey="diffs" for="node" transformer="net.itransformers.topologyviewer.nodetooltip.HTMLCSVNodeTooltipTransformer"/>
            <tooltip dataKey="diffs" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
        </filter>

        <!--In this case is presented a view only in site with Cisco and Default type Devices-->
        <filter name="deviceType=CISCO">
            <include dataKey="deviceType" dataValue="CISCO" for="node"/>
            <include dataKey="method" dataValue="NEXT_HOP" for="edge"/>
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <include dataKey="method" dataValue="Slash30" for="edge"/>
            <include dataKey="method" dataValue="Slash31" for="edge"/>
            <include dataKey="method" dataValue="MAC" for="edge"/>
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_STATIC_ROUTE" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only LLDP view -->
        <filter name="LLDP">
            <include dataKey="method" dataValue="LLDP" for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only CDP view -->
        <filter name="CDP">
            <include dataKey="method" dataValue="CDP" for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only an OSPF view -->
        <filter name="OSPF">
            <include dataKey="method" dataValue="OSPF" for="edge"/>
            <include dataKey="method" dataValue="c_OSPF" for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only an ISIS view -->
        <filter name="ISIS">
            <include dataKey="method" dataValue="ISIS" for="edge"/>
            <include dataKey="method" dataValue="c_ISIS" for="edge"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <!--In this case is presented a only BGP view -->
        <filter name="BGP">
            <include dataKey="method" dataValue="BGP" for="edge"/>
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>

        </filter>
        <filter name="IPLinkLayer-reachable">
            <include dataKey="ipLink" dataValue="YES" for="edge"/>
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
        </filter>
        <filter name="IPLinkLayer-reachable+unreachable">
            <include dataKey="ipLink" dataValue="YES" for="edge"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
        </filter>
        <!--In this case is presented a data+IPlink view with devices that respond to SNMP-->

        <filter name="Data+IPLinkLayer-reachable">
            <include dataKey="deviceStatus" dataValue="discovered" for="node"/>
            <include dataKey="ipLink" dataValue="YES" for="edge"/>
            <include dataKey="dataLink" dataValue="YES" for="edge"/>
            <tooltip dataKey="deviceInfo" for="node"/>
            <tooltip dataKey="edgeTooltip" for="edge" transformer="net.itransformers.topologyviewer.edgetooltip.HTMLCSVEdgeTooltipTransformer"/>
